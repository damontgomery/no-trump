{
  "name": "No Trump",
  "version": "0.1",
  "description": "Get rid of images of Trump and others from the internet.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["hide.css"],
      "js": ["hide.js"]
    }
  ],
  "icons": {
    "128": "128-icon.png"
  }
}
